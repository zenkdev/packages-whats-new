{
  "name": "packages-whats-new",
  "version": "0.0.2",
  "description": "What's new in your NPM packages",
  "main": "dist/index.js",
  "bin": {
    "packages-whats-new": "./dist/index.js"
  },
  "scripts": {
    "start": "ts-node --project ./tsconfig.json ./src/index.ts -d \"C:\\dev\\zp-front\" --to 2022-02-24",
    "build": "tsc --project ./tsconfig.json",
    "test": "test",
    "prettier": "prettier --write \"./src/{,!(node_modules|dist|build|coverage)/**/}*.{js,ts,json}\"",
    "pre-commit": "lint-staged",
    "lint": "tsc --noEmit && eslint ./src/**/*.{js,ts}",
    "lint:fix": "eslint --fix ./src/**/*.{js,ts}"
  },
  "author": "AlexanderS",
  "license": "MIT",
  "devDependencies": {
    "@types/clear": "0.1.2",
    "@types/cli-progress": "3.11.0",
    "@types/figlet": "1.5.4",
    "@types/node": "16.11.45",
    "@types/yargs": "17.0.10",
    "@typescript-eslint/eslint-plugin": "5.43.0",
    "@typescript-eslint/parser": "5.43.0",
    "eslint": "8.20.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "26.6.0",
    "eslint-plugin-jsx-a11y": "6.6.0",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "ts-node": "10.9.1",
    "typescript": "4.7.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit"
    }
  },
  "dependencies": {
    "@supercharge/promise-pool": "^2.0.0",
    "chalk": "4.1.2",
    "clear": "^0.1.0",
    "cli-progress": "^3.9.0",
    "figlet": "^1.5.0",
    "package-json": "7.0.0",
    "yargs": "^17.0.0"
  }
}
