{
  "name": "packages-whats-new",
  "version": "0.0.1",
  "description": "What's new in your NPM packages",
  "main": "dist/index.js",
  "bin": {
    "packages-whats-new": "./dist/index.js"
  },
  "scripts": {
    "start": "ts-node --project ./tsconfig.json ./src/index.ts",
    "build": "tsc --project ./tsconfig.json",
    "test": "test",
    "prettier": "prettier --write \"./src/{,!(node_modules|dist|build|coverage)/**/}*.{js,ts,json}\"",
    "pre-commit": "lint-staged",
    "lint": "tsc --noEmit && eslint ./src/**/*.{js,ts}",
    "lint:fix": "eslint --fix ./src/**/*.{js,ts}"
  },
  "author": "AlexanderS",
  "license": "MIT",
  "devDependencies": {
    "@types/clear": "^0.1.1",
    "@types/cli-progress": "^3.8.0",
    "@types/figlet": "^1.2.1",
    "@types/node": "^14.14.22",
    "@types/yargs": "^15.0.13",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "eslint": "^7.19.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit"
    }
  },
  "dependencies": {
    "@supercharge/promise-pool": "^1.6.0",
    "chalk": "^4.1.0",
    "clear": "^0.1.0",
    "cli-progress": "^3.9.0",
    "figlet": "^1.5.0",
    "package-json": "^6.5.0",
    "read-package-json": "^3.0.0",
    "yargs": "^16.2.0"
  }
}
