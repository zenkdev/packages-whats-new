{
  "name": "packages-whats-new",
  "version": "0.0.2",
  "description": "What's new in your NPM packages",
  "main": "dist/index.js",
  "bin": {
    "packages-whats-new": "./dist/index.js"
  },
  "scripts": {
    "start": "ts-node --project ./tsconfig.json ./src/index.ts",
    "build": "tsc --project ./tsconfig.json",
    "test": "test",
    "prettier": "prettier --write \"./src/{,!(node_modules|dist|build|coverage)/**/}*.{js,ts,json}\"",
    "pre-commit": "lint-staged",
    "lint": "tsc --noEmit && eslint ./src/**/*.{js,ts}",
    "lint:fix": "eslint --fix ./src/**/*.{js,ts}"
  },
  "author": "AlexanderS",
  "license": "MIT",
  "devDependencies": {
    "@types/clear": "0.1.2",
    "@types/cli-progress": "3.9.2",
    "@types/figlet": "1.5.4",
    "@types/node": "16.11.26",
    "@types/yargs": "17.0.10",
    "@typescript-eslint/eslint-plugin": "5.16.0",
    "@typescript-eslint/parser": "5.16.0",
    "eslint": "8.11.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jest": "26.1.2",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "7.0.4",
    "lint-staged": "12.3.7",
    "prettier": "2.6.0",
    "ts-node": "10.7.0",
    "typescript": "4.6.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit"
    }
  },
  "dependencies": {
    "@supercharge/promise-pool": "^2.0.0",
    "chalk": "^5.0.0",
    "clear": "^0.1.0",
    "cli-progress": "^3.9.0",
    "figlet": "^1.5.0",
    "package-json": "^7.0.0",
    "read-package-json": "^5.0.0",
    "yargs": "^17.0.0"
  }
}
